# CFO Financial System - Frontend Environment Configuration
# Copy this file to .env.local for development or set in Vercel for production

# API Base URL - Leave empty to use proxy in development
# For production, set to your backend API URL
VITE_API_BASE_URL=

# Application Settings
VITE_APP_NAME=CFO Financial System
VITE_APP_VERSION=1.0.0

# Feature Flags (optional)
VITE_ENABLE_AI_INSIGHTS=true
VITE_ENABLE_FORECASTING=true
VITE_ENABLE_DARK_MODE=true
